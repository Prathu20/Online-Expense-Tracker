<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>userRegister</servlet-name>
  	<display-name>userRegister</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlet.userRegister</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveExpenseServlet</servlet-name>
  	<display-name>SaveExpenseServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlet.SaveExpenseServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateExpenseServlet</servlet-name>
  	<display-name>UpdateExpenseServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlet.UpdateExpenseServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteExpenseServlet</servlet-name>
  	<display-name>DeleteExpenseServlet</display-name>
  	<description></description>
  	<servlet-class>com.demo.servlet.DeleteExpenseServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>userRegister</servlet-name>
  	<url-pattern>/userRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveExpenseServlet</servlet-name>
  	<url-pattern>/SaveExpenseServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateExpenseServlet</servlet-name>
  	<url-pattern>/UpdateExpenseServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteExpenseServlet</servlet-name>
  	<url-pattern>/DeleteExpenseServlet</url-pattern>
  </servlet-mapping>
</web-app>